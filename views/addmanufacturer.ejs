<% include partials/header %>
<h1>Add Manufactuer</h1>
<div class="card bg-secondary text-center card-form" style="opacity: 0.8; width: 50%; margin:auto">
          <class class="card-body">
            <h2><strong>Add Manufacturer</strong></h2>
            <div class="user-img">
              <img src="../img/register/user.png">
            </div>
            <form action="/admin/addmanufacturer" method="post" >
              <div class="form-group form-row">
                <div class="col">
                  <input class="form-control form-control-lg" type="text" name="name" value="" placeholder="Name">
                </div>

                <div class="col">
                    <input class="form-control form-control-lg" type="text" name="address" value="" placeholder="Address">
                </div>


              </div>

              <div class="form-group form-row">
                <div class="col">
                           <input class="form-control form-control-lg" type="text" name="phone" placeholder="Phone">
                </div>
                <div class="col">
                           <input class="form-control form-control-lg" type="text" name="licenseNumber" placeholder="License Number">
                </div>
              </div>
                    <div class="form-group form-row">
                <button class="btn btn-outline-light btn-block" type="submit" name="button">Add Manufactuer</button>
              </div>





            </form>
          </class>
        </div>
